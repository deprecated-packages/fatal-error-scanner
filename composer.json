{
    "name": "migrify/fatal-error-scanner",
    "description": "Scans fatal error in code, useful for type conflicts analysis",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "symfony/finder": "^4.4|^5.0",
        "symplify/package-builder": "^7.2",
        "symplify/autowire-array-parameter": "^7.2",
        "symplify/smart-file-system": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\FatalErrorScanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\FatalErrorScanner\\Tests\\": "tests"
        }
    }
}
